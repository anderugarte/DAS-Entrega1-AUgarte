<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000">

    <TextView
        android:id="@+id/tCumple"
        android:layout_width="345dp"
        android:layout_height="31dp"
        android:fontFamily="@font/contrail_one"
        android:text="Fecha de Nacimiento"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.958"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tPassword"
        app:layout_constraintVertical_bias="0.054" />

    <EditText
        android:id="@+id/tpUsername"
        android:layout_width="297dp"
        android:layout_height="45dp"
        android:backgroundTint="#F4F3F3"
        android:ems="10"
        android:fontFamily="@font/contrail_one"
        android:inputType="textPersonName"
        android:textAppearance="@style/TextAppearance.AppCompat.Display4"
        android:textColor="#FFFFFF"
        android:textCursorDrawable="@color/design_default_color_error"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tpUsername2"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/tMostrarCumple"
        android:layout_width="297dp"
        android:layout_height="45dp"
        android:backgroundTint="#FFFFFF"
        android:clickable="true"
        android:focusable="false"
        android:fontFamily="@font/contrail_one"
        android:textColor="#FFFFFF"
        android:textCursorDrawable="@color/design_default_color_error"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.852"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tCumple"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tContrasena"
        android:layout_width="345dp"
        android:layout_height="31dp"
        android:fontFamily="@font/contrail_one"
        android:text="ContraseÃ±a"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.958"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.042" />

    <Button
        android:id="@+id/bRegistrarse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#FF0000"
        android:fontFamily="@font/contrail_one"
        android:text="Registrarse"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.763"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tMostrarCumple"
        app:layout_constraintVertical_bias="0.257" />

    <TextView
        android:id="@+id/tFilmometryTituloR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/contrail_one"
        android:text="Filmometry"
        android:textColor="#FF0000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.738"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bRegistrarse"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tNombre"
        android:layout_width="345dp"
        android:layout_height="31dp"
        android:fontFamily="@font/contrail_one"
        android:text="Nombre"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.042" />

    <TextView
        android:id="@+id/tApellidos"
        android:layout_width="345dp"
        android:layout_height="31dp"
        android:fontFamily="@font/contrail_one"
        android:text="Apellidos"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tpNombre"
        app:layout_constraintVertical_bias="0.054" />

    <TextView
        android:id="@+id/tpUsername2"
        android:layout_width="345dp"
        android:layout_height="31dp"
        android:fontFamily="@font/contrail_one"
        android:text="Username"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tpApellidos"
        app:layout_constraintVertical_bias="0.103" />

    <EditText
        android:id="@+id/tpNombre"
        android:layout_width="299dp"
        android:layout_height="45dp"
        android:backgroundTint="#F4F3F3"
        android:drawableTint="#FF0000"
        android:ems="10"
        android:fontFamily="@font/contrail_one"
        android:foregroundTint="#FFFFFF"
        android:inputType="textPersonName"
        android:textAppearance="@style/TextAppearance.AppCompat.Display4"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FF0000"
        android:textColorHint="#FF0000"
        android:textColorLink="#FF0000"
        android:textCursorDrawable="@color/design_default_color_error"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tNombre"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/tPassword"
        android:layout_width="297dp"
        android:layout_height="45dp"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:inputType="textPassword"
        android:textColor="#FFFFFF"
        android:textCursorDrawable="@color/design_default_color_error"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.846"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tContrasena"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/tpApellidos"
        android:layout_width="299dp"
        android:layout_height="45dp"
        android:backgroundTint="#F4F3F3"
        android:ems="10"
        android:fontFamily="@font/contrail_one"
        android:inputType="textPersonName"
        android:textAppearance="@style/TextAppearance.AppCompat.Display4"
        android:textColor="#FFFFFF"
        android:textCursorDrawable="@color/design_default_color_error"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tApellidos"
        app:layout_constraintVertical_bias="0.024" />

</androidx.constraintlayout.widget.ConstraintLayout>